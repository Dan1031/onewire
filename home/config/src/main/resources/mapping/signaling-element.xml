<?xml version="1.0"?>

<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" 
"http://castor.org/mapping.dtd">

<mapping>
  <description>Mapping for single signaling element configuration</description>

  <class name="by.bsu.onewire.common.modules.signaling.SignalingElement">
    <map-to xml="signaling-element" />

    <field name="id" type="long">
      <bind-xml name="id" node="attribute" />
    </field>
    <field name="enabled" type="boolean">
      <bind-xml name="enabled" node="attribute" />
    </field>
    <field name="title" type="string">
      <bind-xml name="title" node="element" />
    </field>
    <field name="keyAddressString" type="string">
      <bind-xml name="keyAddress" node="element" />
    </field>
    <field name="labelAddressString" type="string">
      <bind-xml name="labelAddress" node="element" />
    </field>
  </class>
</mapping>